<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">


    
    <style name="widget_listview">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:divider">#B1B1B1</item>
        <item name="android:dividerHeight">1.0dip</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:scrollingCache">false</item>
    </style>

    <style name="head_layout">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">50dip</item>
        <item name="android:background">#0B1E3E</item>
    </style>

    <style name="DialogStyle">
        <item name="android:windowBackground">@color/transparent</item>  
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="input_layout">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:background">@drawable/input</item>
    </style>

    <style name="title_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">21sp</item>
    </style>

    <!--
    <style name="left_btn">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:background">@drawable/btn_back</item>
    </style>
    -->

    <style name="btn">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:background">@drawable/btn</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">17sp</item>
    </style>

    <style name="line">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">#D1D1D1</item>
    </style>

    <style name="left_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="right_ed">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:background">@null</item>
        <item name="android:layout_marginLeft">4dp</item>
    </style>

    <style name="tb_row_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">40.0dip</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="tb_row_et">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">40.0dip</item>
    </style>

    <style name="Dialog_Fullscreen">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <!-- 自定义标题样式 -->
    <style name="AllTitleBackground">
        <item name="android:background">#1E90FF</item>
    </style>

    <style name="myTitle" parent="android:Theme">
        <item name="android:windowTitleSize">50dp</item>
        <item name="android:windowTitleBackgroundStyle">@style/AllTitleBackground</item>
    </style>

    <!-- 分割线 -->
    <style name="vertical_layout">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:background">#dadada</item>
    </style>

    <style name="horizontal_layout">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">#dadada</item>
    </style>

    <!-- 字体 -->
    <style name="TV_Form">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginLeft">5dp</item>
    </style>

    <style name="TV_Form_M">
        <item name="android:textSize">17.0sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginLeft">4dp</item>
    </style>

    <style name="TV_Form_L">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/grgray</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:gravity">right</item>
        <item name="android:layout_marginRight">3dp</item>
    </style>

    <style name="persion_row" parent="base">
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:layout_marginLeft">0dp</item>
        <item name="android:background">@color/white</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_width">fill_parent</item>
    </style>

    <style name="base">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="personal" parent="base">
        <item name="android:textSize">16sp</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:paddingLeft">10dp</item>
    </style>

    <style name="person_line" parent="@style/base">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:background">@color/white</item>
        <item name="android:src">@drawable/black_line</item>
    </style>

    <!-- 星级评价定义  android:layout_gravity="center_vertical" -->
    <style name="roomRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/ratingbar_drawable</item>
        <item name="android:minHeight">30dp</item>
        <item name="android:maxHeight">30dp</item>
    </style>

    <!-- 星级评价定义  android:layout_gravity="center_vertical" -->
    <style name="gradeBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/ratingbar_small_selector</item>
        <item name="android:minHeight">16dp</item>
        <item name="android:maxHeight">16dp</item>
    </style>

    <style name="editTextStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:paddingBottom">11dp</item>
        <item name="android:paddingTop">11dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:background">@drawable/bg_my_input</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14sp</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="editTextViewStyle">
        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:background">@drawable/bg_my_input_pre</item>
        <item name="android:textColor">#888888</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="MyButtonStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:padding">10dp</item>
        <item name="android:background">@drawable/btn_common_selector</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="CommonItemStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">#888888</item>
        <item name="android:padding">12dp</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="CommonItemLayoutStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:background">@drawable/item_common_selector</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="SmallRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">22px</item>
        <item name="android:maxHeight">22px</item>
        <item name="android:numStars">5</item>
        <item name="android:stretchMode">none</item>
        <item name="android:stepSize">0.5</item>
        <item name="android:isIndicator">true</item>
        <item name="android:progressDrawable">@drawable/ratingbar_small_selector</item>
    </style>

    <style name="MiddleRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">38px</item>
        <item name="android:maxHeight">38px</item>
        <item name="android:numStars">5</item>
        <item name="android:stepSize">0.5</item>
        <item name="android:progressDrawable">@drawable/ratingbar_middle_selector</item>
    </style>

    <style name="BigRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">48px</item>
        <item name="android:maxHeight">48px</item>
        <item name="android:numStars">5</item>
        <item name="android:stepSize">0.5</item>
        <item name="android:progressDrawable">@drawable/ratingbar_big_selector</item>
    </style>

    <style name="MyDialog" parent="@android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/blue</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="ListItem1Style">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingRight">8dp</item>
    </style>

    <style name="ListItem2Style">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:padding">10dp</item>
        <item name="android:background">@drawable/my_list_item_selector</item>
    </style>

    <style name="RadioButtonStyles">
        <item name="android:button">@drawable/radio_button_selector</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:minHeight">35dp</item>
        <item name="android:maxHeight">35dp</item>
    </style>
    
    
    <style name="myProgressBarStyleLarge" >
　　		<item name="android:indeterminateDrawable">@drawable/progress_large</item>
	</style>

</resources>